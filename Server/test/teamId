<script type="text/javascript">
(function(){
  var el = $$('.ad_floating_bar')[0];
  if (el) {
    var cookie_name = 'sw_no_ad_floating_bar';
    if (!Cookie.get(cookie_name)) {
      if (typeof floatingBarAd != 'undefined') {
        var text = floatingBarAd();
        document.write(text);
        el.setStyle({display:'block'});
          Element.select(el, '.close')[0].observe('click', function(evt){
            el.style.display = 'none';

            var date = new Date();
            date.setTime(date.getTime()+(24*60*60*1000)); // 24h

            Cookie.set(cookie_name, 1, {expires: date.toUTCString(), path: '/', domain: Config.get('base_domain')});
            Event.stop(evt);
          });
          var counter = 20;

          var run = function() {
            if (counter--) {
              setTimeout(function(){
                el.setStyle({display:'block'});
                var els = $('floating_bar_adtech_container').childElements();
                var found = false;
                els.each(function(adtech_el) {
                  if (found) return;
                  if (adtech_el.nodeName.toLowerCase() !='script' && adtech_el.nodeName.toLowerCase() !='noscript') {
                    found = true;
                    if (adtech_el.getWidth() < 800) {
                      el.setStyle({display:'none'});
                    }
                  }
                });
                if (!found) {
                  el.setStyle({display:'none'});
                  run();
                }

              }, 200);
            }
          };
          run();

      }

    }
  }
})();
</script>
